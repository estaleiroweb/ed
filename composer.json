{
	"name": "estaleiroweb/ed",
	"description": "Easy Data Framework. An easy way to get a CRUD form added a filter grid builded by reverse engineer and other a lot of tools.",
	"type": "library",
	"license": "MIT",
	"version": "1.0.0",
	"autoload": {
		"files": [
			"main.php"
		],
		"psr-4": {
			"EstaleiroWeb\\ED\\": "src/",
			"EstaleiroWeb\\Cache\\": "cache/"
		}
	},
	"authors": [{
		"name": "estaleiroweb",
		"email": "helbert@estaleiroweb.com.br"
	}],
	"require": {
		"php": ">=5.4",
		"ext-ctype": "*",
		"ext-dom": "*",
		"ext-fileinfo": "*",
		"ext-gd": "*",
		"ext-iconv": "*",
		"ext-libxml": "*",
		"ext-mbstring": "*",
		"ext-mysqli": "*",
		"ext-pdo": "*",
		"ext-pdo_mysql": "*",
		"ext-session": "*",
		"ext-simplexml": "*",
		"ext-xml": "*",
		"ext-xmlreader": "*",
		"ext-xmlwriter": "*",
		"ext-zip": "*",
		"ext-zlib": "*",
		"estaleiroweb/traits": "*@dev",
		"phpseclib/phpseclib": "*"
	},
	"require-dev": {
		"phpunit/phpunit": "^4.8.18"
	}
}
